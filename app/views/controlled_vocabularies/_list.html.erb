<turbo-frame id="controlled-vocabularies-list">
  <% controlled_vocabularies.each do |vocabulary| %>
    <li>
      <a
        class="dropdown-item text-wrap w-100"
        style="white-space: normal;"
        href="/entity?uri=<%= CGI.escape(vocabulary[:uri]) %>"
      ><%= vocabulary[:label] %></a>
    </li>
  <% end %>
</turbo-frame>
